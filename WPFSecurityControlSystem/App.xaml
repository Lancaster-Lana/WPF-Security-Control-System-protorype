             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>        
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <!-- Set default skin -->
                <ResourceDictionary Source="Themes\Skins\BureauBlue.xaml"/>
